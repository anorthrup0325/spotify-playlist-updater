{
  "name": "spotify-playlist-updater",
  "version": "1.0.0",
  "description": "Update playlists with new music from the artist of the playlist",
  "main": "dist/server.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy:assets": "npm-run-all copy:images copy:views",
    "copy:images": "ts-node utils/copy-images.ts",
    "copy:views": "ts-node utils/copy-views.ts",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "build": "npm-run-all clean lint tsc copy:assets",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ejs,png,ts --exec npm run dev:start",
    "start": "node .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "ejs-lint": "ejslint"
  },
  "author": "Anthony Northrup",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.0.2",
    "express": "^4.17.1",
    "lowdb": "^1.0.0",
    "query-string": "^6.11.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/bootstrap": "^4.3.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/ejs": "^3.0.1",
    "@types/express": "^4.17.4",
    "@types/fs-extra": "^8.1.0",
    "@types/jquery": "^3.3.34",
    "@types/lowdb": "^1.0.9",
    "@types/node": "^13.9.8",
    "@types/query-string": "^6.3.0",
    "@types/request": "^2.48.4",
    "@types/request-promise": "^4.1.46",
    "@types/shelljs": "^0.8.7",
    "@types/uuid": "^7.0.2",
    "ejs-lint": "^1.0.1",
    "fs-extra": "^9.0.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.3",
    "ts-node": "^8.8.1",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3"
  }
}
